	"my_qs": {
		"prefix": "my_qs",
		"body": [
			"document.querySelector('$1')"
		],
		"description": "快速生成QS"
	},
	"my_qsa": {
		"prefix": "my_qsa",
		"body": [
			"document.querySelectorAll('$1')"
		],
		"description": "快速生成QSA"
	},
	"my_ajax_JQ": {
		"prefix": "my_ajax_JQ",
		"body": [
			"var formData = new FormData() //上传文件需要的对象",
			"$.ajax({",
					"url:'需要传入的地址(php文件)',",
					"data:'name='+$(''), //需要传输的值",
					"type:'get或者post',",
					"contentType: false, //JQuery 内部不帮助我们设置请求头",	
		 			"processData: false, //不去格式化发送数据",
					"success:function(data){},error:function(){} //回调函数",
			"})"
		],
		"description": "快速生成JQ AJAX"
	},
	"my_node": {
		"prefix": "my_node",
		"body": [
			"res.writeHead(200,{'Content-Type':'text/html或者plain(是普通文本);charset=utf-8'});"
		],
		"description": "快速生成nodejs的请求头"
	}
	
	
